# NetBSD Stuff

Host wip.pkgsrc.org
     PubKeyAuthentication yes
     IdentityFile ~/.ssh/id_rsa

Host cvs.netbsd.org
     HostName cvs.netbsd.org
     PubKeyAuthentication yes
     IdentityFile ~/.ssh/id_rsa_netbsd
     User fox
     ForwardAgent true

Host ftp.NetBSD.org # For pkgsrc upload-distfiles
     HostName ftp.netbsd.org
     PubKeyAuthentication yes
     IdentityFile ~/.ssh/id_rsa_netbsd
     User fox

Host netbsd-mail # mail.netbsd.org (mail server)
     HostName homeworld.netbsd.org
     PubKeyAuthentication yes
     IdentityFile ~/.ssh/id_rsa_netbsd
     User fox

Host netbsd-ftp # {ftp,sup}.NetBSD.org (ftp/sup/rsync server)
     HostName morden.netbsd.org
     PubKeyAuthentication yes
     IdentityFile ~/.ssh/id_rsa_netbsd
     User fox

Host netbsd-www # {www,gnats,wiki}.NetBSD.org (http/gnats server)
     HostName mollari.netbsd.org
     PubKeyAuthentication yes
     IdentityFile ~/.ssh/id_rsa_netbsd
     User fox

